<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Default Jewelry Types with Spanish keywords -->

        <record id="jewelry_type_ring" model="jewelry.type">
            <field name="name">Anillo</field>
            <field name="sequence">10</field>
            <field name="keywords">anillo, alianza, sortija, solitario, compromiso, boda, sello</field>
        </record>

        <record id="jewelry_type_earrings" model="jewelry.type">
            <field name="name">Pendientes</field>
            <field name="sequence">20</field>
            <field name="keywords">pendiente, pendientes, arete, aretes, aro, aros, dormilona, dormilonas, criolla, criollas</field>
        </record>

        <record id="jewelry_type_necklace" model="jewelry.type">
            <field name="name">Collar</field>
            <field name="sequence">30</field>
            <field name="keywords">collar, collares, cadena, cadenas, gargantilla, riviere</field>
        </record>

        <record id="jewelry_type_pendant" model="jewelry.type">
            <field name="name">Colgante</field>
            <field name="sequence">40</field>
            <field name="keywords">colgante, colgantes, medalla, medallas, medallon, cruz, crucifijo, dije</field>
        </record>

        <record id="jewelry_type_bracelet" model="jewelry.type">
            <field name="name">Pulsera</field>
            <field name="sequence">50</field>
            <field name="keywords">pulsera, pulseras, esclava, brazalete, semanario, rigida</field>
