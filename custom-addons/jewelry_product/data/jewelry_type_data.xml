        <record id="jewelry_type_watch" model="jewelry.type">
            <field name="name">Reloj</field>
            <field name="sequence">60</field>
            <field name="keywords">reloj, relojes, cronografo, automatico</field>
        </record>

        <record id="jewelry_type_brooch" model="jewelry.type">
            <field name="name">Broche</field>
            <field name="sequence">70</field>
            <field name="keywords">broche, broches, alfiler, imperdible, pin</field>
        </record>

        <record id="jewelry_type_set" model="jewelry.type">
            <field name="name">Conjunto</field>
            <field name="sequence">80</field>
            <field name="keywords">conjunto, juego, aderezo, set, parure</field>
        </record>

        <record id="jewelry_type_coin" model="jewelry.type">
            <field name="name">Moneda</field>
            <field name="sequence">90</field>
            <field name="keywords">moneda, monedas, onza, lingote, bullion, krugerrand, maple, filarmonica, soberano</field>
        </record>

        <record id="jewelry_type_scrap" model="jewelry.type">
            <field name="name">Chatarra</field>
            <field name="sequence">100</field>
            <field name="keywords">chatarra, roto, rotos, pieza, piezas, fundicion, fundir, metal, fragmento</field>
        </record>

        <record id="jewelry_type_other" model="jewelry.type">
            <field name="name">Otros</field>
            <field name="sequence">999</field>
            <field name="keywords"></field>
        </record>

    </data>
</odoo>
